type Banco {
	id: ID!
	nombre: String
	clave: String!
	fechaRegistro: String
}

type Cliente {
	id: ID!
	nombre: String
	correo: String!
	apellidoPaterno: String
	apellidoMaterno: String
}

type Query {
	bancos(registros: Int): [Banco]
	banco(id: ID): Banco
	clientes(registros: Int): [Cliente]
	cliente(id: ID): Cliente
}

type Mutation {
	registrarBanco(
		nombre: String!	
		clave: String!
		fechaRegistro: String
	): Banco
	
	registrarCliente(
		nombre: String!
		correo: String!
		apellidoPaterno: String
		apellidoMaterno: String
	): Cliente
}
